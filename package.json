{
  "name": "byte-encodings",
  "version": "1.0.10",
  "description": "Utilities for encoding and decoding common formats like hex, base64, and varint. Ported from Deno's @std/encoding.",
  "keywords": [
    "encoding",
    "ascii85",
    "base32",
    "base58",
    "base64",
    "base64url",
    "hex",
    "varint"
  ],
  "author": {
    "name": "Deno authors",
    "url": "https://deno.com/"
  },
  "homepage": "https://github.com/dahlia/byte-encodings",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dahlia/byte-encodings.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dahlia/byte-encodings/issues"
  },
  "type": "module",
  "main": "./dist/mod.cjs",
  "module": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mod.d.ts",
        "default": "./dist/mod.js"
      },
      "require": {
        "types": "./dist/mod.d.cts",
        "default": "./dist/mod.cjs"
      }
    },
    "./ascii85": {
      "import": {
        "types": "./dist/ascii85.d.ts",
        "default": "./dist/ascii85.js"
      },
      "require": {
        "types": "./dist/ascii85.d.cts",
        "default": "./dist/ascii85.cjs"
      }
    },
    "./base32": {
      "import": {
        "types": "./dist/base32.d.ts",
        "default": "./dist/base32.js"
      },
      "require": {
        "types": "./dist/base32.d.cts",
        "default": "./dist/base32.cjs"
      }
    },
    "./unstable-base32": {
      "import": {
        "types": "./dist/unstable_base32.d.ts",
        "default": "./dist/unstable_base32.js"
      },
      "require": {
        "types": "./dist/unstable_base32.d.cts",
        "default": "./dist/unstable_base32.cjs"
      }
    },
    "./unstable-base32-stream": {
      "import": {
        "types": "./dist/unstable_base32_stream.d.ts",
        "default": "./dist/unstable_base32_stream.js"
      },
      "require": {
        "types": "./dist/unstable_base32_stream.d.cts",
        "default": "./dist/unstable_base32_stream.cjs"
      }
    },
    "./base58": {
      "import": {
        "types": "./dist/base58.d.ts",
        "default": "./dist/base58.js"
      },
      "require": {
        "types": "./dist/base58.d.cts",
        "default": "./dist/base58.cjs"
      }
    },
    "./base64": {
      "import": {
        "types": "./dist/base64.d.ts",
        "default": "./dist/base64.js"
      },
      "require": {
        "types": "./dist/base64.d.cts",
        "default": "./dist/base64.cjs"
      }
    },
    "./unstable-base64": {
      "import": {
        "types": "./dist/unstable_base64.d.ts",
        "default": "./dist/unstable_base64.js"
      },
      "require": {
        "types": "./dist/unstable_base64.d.cts",
        "default": "./dist/unstable_base64.cjs"
      }
    },
    "./unstable-base64-stream": {
      "import": {
        "types": "./dist/unstable_base64_stream.d.ts",
        "default": "./dist/unstable_base64_stream.js"
      },
      "require": {
        "types": "./dist/unstable_base64_stream.d.cts",
        "default": "./dist/unstable_base64_stream.cjs"
      }
    },
    "./base64url": {
      "import": {
        "types": "./dist/base64url.d.ts",
        "default": "./dist/base64url.js"
      },
      "require": {
        "types": "./dist/base64url.d.cts",
        "default": "./dist/base64url.cjs"
      }
    },
    "./hex": {
      "import": {
        "types": "./dist/hex.d.ts",
        "default": "./dist/hex.js"
      },
      "require": {
        "types": "./dist/hex.d.cts",
        "default": "./dist/hex.cjs"
      }
    },
    "./unstable-hex": {
      "import": {
        "types": "./dist/unstable_hex.d.ts",
        "default": "./dist/unstable_hex.js"
      },
      "require": {
        "types": "./dist/unstable_hex.d.cts",
        "default": "./dist/unstable_hex.cjs"
      }
    },
    "./unstable-hex-stream": {
      "import": {
        "types": "./dist/unstable_hex_stream.d.ts",
        "default": "./dist/unstable_hex_stream.js"
      },
      "require": {
        "types": "./dist/unstable_hex_stream.d.cts",
        "default": "./dist/unstable_hex_stream.cjs"
      }
    },
    "./varint": {
      "import": {
        "types": "./dist/varint.d.ts",
        "default": "./dist/varint.js"
      },
      "require": {
        "types": "./dist/varint.d.cts",
        "default": "./dist/varint.cjs"
      }
    }
  },
  "devDependencies": {
    "@std/assert": "jsr:^1.0.13",
    "@std/bytes": "jsr:^1.0.6",
    "@std/streams": "jsr:^1.0.9",
    "@types/node": "^22.15.21",
    "deno": "^2.3.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepack": "tsup",
    "prepublish": "tsup",
    "test": "vitest",
    "format": "deno fmt",
    "format:watch": "deno fmt --watch",
    "dev": "concurrently \"tsup --watch\" \"deno fmt --watch\""
  }
}
